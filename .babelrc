{
  "presets": [
    "es2015",
    "stage-2",
    "vue-app"
  ],
  "plugins": [
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-vue-jsx",
    "transform-runtime"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs", "dynamic-import-node"]
    }
  }
}
